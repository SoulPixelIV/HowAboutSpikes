        audio_play_sound(click_snd, 1, false);
    }
    if (selectedElement &gt; -1)
    {
        selectedElement--;
    }
    if (selectedElement == -1)
    {
        for (x = 0; x &lt; array_length_1d(menuElements); x++)
        {
            if (menuElements[x] == "")
            {
                selectedElement = x - 1;
                exit;
            }
            if (x == array_length_1d(menuElements) - 1 &amp;&amp; menuElements[x] != "")
            {
